class Solution {
public:
    void find(int n,int index,vector<string>&ans,int mask,string s){
        if(index==n){
            ans.push_back(s);
            return ;
        }
        for(int i=1;i<=n;i++){
            if((mask>>i)&1)continue;
            find(n,index+1,ans,mask|(1<<i),s+char(i+'0'));
        }
    }
    string getPermutation(int n, int k) {
        vector<string> ans;
        find(n,0,ans,0,"");
        return ans[k-1];
    }
};
